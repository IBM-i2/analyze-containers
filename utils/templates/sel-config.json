{
    "serverPort": 3000,
    "sslSettings": {
        "enabled": false,
        "port": 3001,
        "keyFile": "<server.key>",
        "certFile": "<server.cer>",
        "certChecking" : {
            "enabled": false,
            "ca": ["<CA.cer>"],
            "ocspCheckingEnabled": false,
            "ocspFallbackResponder": "http://<OCSP_SERVER>:2560"
        }
    },
    "logSettings": {
        "level": "info",
        "logDir": "./logs"
    },
    "jwt": {
        "required": false,
        "secret": "<jwt_secret>" 
    }
}